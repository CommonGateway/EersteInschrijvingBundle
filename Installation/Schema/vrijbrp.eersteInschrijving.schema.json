{
    "$id": "https://vrijbrp.nl/schemas/vrijbrp.eersteInschrijving.schema.json",
    "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
    "version": "0.2.1",
    "title": "EersteInschrijving",
    "type": "object",
    "properties": {
        "referentienummer": {
            "type": "string",
            "description": "Referentienummer"
        },
        "geslacht": {
            "type": "string",
            "description": "Geslacht",
            "enum": [
                "V",
                "M"
            ],
            "required": true
        },
        "voornamen": {
            "type": "string",
            "description": "Voornamen",
            "required": true
        },
        "voorvoegsel": {
            "type": "string",
            "description": "Voorvoegsel/tussenvoegsel"
        },
        "achternaam": {
            "type": "string",
            "description": "Achternaam",
            "required": true
        },
        "geboortedatum": {
            "type": "string",
            "description": "Geboortedatum",
            "required": true
        },
        "geboorteplaats": {
            "type": "string",
            "description": "Geboorteplaats",
            "required": true
        },
        "geboorteland": {
            "type": "string",
            "description": "Geboorteland",
            "required": true
        },
        "land van vorig adres": {
            "type": "string",
            "description": "Land van vorig adres"
        },
        "land_van_herkomst": {
          "type": "string",
          "description": "Land van vorig adres"
        },
        "e-mail": {
            "type": "string",
            "description": "E-mail adres",
            "required": true
        },
        "telefoon": {
            "type": "string",
            "description": "Telefoonnummer"
        },
        "postcode": {
            "type": "string",
            "description": "Postcode (nieuw adres)",
            "required": true
        },
        "huisnummer": {
            "type": "string",
            "description": "Huisnummer (nieuw adres)",
            "required": true
        },
        "huisletter": {
            "type": "string",
            "description": "Huisletter (nieuw adres)"
        },
        "toevoeging": {
            "type": "string",
            "description": "Huisnummertoevoeging (nieuw adres)"
        },
        "straatnaam": {
            "type": "string",
            "description": "Straatnaam (nieuw adres)",
            "required": true
        },
        "woonplaats": {
            "type": "string",
            "description": "Woonplaats (nieuw adres)",
            "required": true
        },
        "datum ingang bewoning": {
            "type": "string",
            "description": "Datum ingang bewoning"
        },
        "opmerkingen": {
            "type": "string",
            "description": "Opmerkingen bij nieuw adres)"
        }
    },
    "required": [
        "geslacht",
        "voornamen",
        "achternaam",
        "geboortedatum",
        "geboorteplaats",
        "geboorteland",
        "e-mail",
        "postcode",
        "huisnummer",
        "straatnaam",
        "woonplaats"
    ]
}
